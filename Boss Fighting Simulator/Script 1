-- Scirptz

AutoAttack.MouseButton1Down:connect(function()
	while true do wait()
local hit = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool")
hit.Attack:FireServer()
hit:Activate()
end
end)

AutoSell.MouseButton1Down:connect(function()
while wait(.1) do
local Target = game:GetService("ReplicatedStorage").Remotes.SellPower;
Target:InvokeServer();
end
end)

GodMode.MouseButton1Down:connect(function()
	game.StarterGui:SetCore("SendNotification", {
	Title = "ðŸ¤¡";
	Text = "TUN TUN TUUUUUUN";
	Icon = "";
	Duration = 5;
})	
while true do
wait()
game.ReplicatedStorage.Remotes.SelfDamage:FireServer(-math.huge)
end
end)

Noob.MouseButton1Down:connect(function()
	debounce = false
game:GetService('RunService').Stepped:connect(function()
    if debounce == false and game.Workspace.Arenas.NoobArena.Boss:FindFirstChildWhichIsA("Model") then
        debounce = true
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Arenas.NoobArena.Boss:FindFirstChildWhichIsA("Model").UpperTorso.CFrame * CFrame.new(0,-8,2)
        debounce = false
    end
end)
end)

Zeus.MouseButton1Down:connect(function()
	game:GetService('RunService').Stepped:connect(function()
    if debounce == false and game.Workspace.Arenas.ZeusArena.Boss:FindFirstChildWhichIsA("Model") then
        debounce = true
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Arenas.ZeusArena.Boss:FindFirstChildWhichIsA("Model").UpperTorso.CFrame * CFrame.new(0,-10,2)
        debounce = false
    end
end)
end)

